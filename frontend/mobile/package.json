{
  "name": "bk-sops-mobile-fe",
  "version": "1.0.0",
  "description": "Project description",
  "author": "luofan",
  "scripts": {
    "dev": "cross-env NODE_ENV=development webpack-dev-server --progress --config ./build/webpack.dev.config.js",
    "build": "cross-env NODE_ENV=production webpack --config ./build/webpack.prod.config.js",
    "clean": "rimraf dist",
    "clean-dll": "rimraf static/lib*",
    "build:analyzer": "better-npm-run build:analyzer",
    "dll": "better-npm-run dll"
  },
  "keywords": [],
  "license": "ISC",
  "betterScripts": {
    "dll": {
      "command": "babel-node build/build-dll.js",
      "env": {
        "NODE_ENV": "production"
      }
    },
    "dev": {
      "command": "babel-node build/check-dll.js && babel-node build/dev-server.js",
      "env": {
        "NODE_ENV": "development"
      }
    },
    "build": {
      "command": "npm run clean-dll && npm run dll && babel-node build/build.js",
      "env": {
        "NODE_ENV": "production"
      }
    },
    "build:analyzer": {
      "command": "npm run clean && npm run clean-dll && npm run dll && babel-node build/build.js",
      "env": {
        "NODE_ENV": "production",
        "npm_config_report": 1
      }
    }
  },
