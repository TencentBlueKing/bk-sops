{
  "root": true,
  "parserOptions": {
    "parser": "babel-eslint"
  },
  "env": {
    "es6": true,
    "browser": true,
    "jquery": true
  },
  "extends": [
    "standard"
  ],
  "rules": {
    "generator-star-spacing": "off",
    "no-debugger": "off",
    "indent": ["error", 4]
  },
  "globals": {
    "jsPlumb": true
  }
}